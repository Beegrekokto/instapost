<% @posts.each do |post| %> 
	<%= image_tag post.user.photo %>
	<%= post.user.name %>
	<%= image_tag post.photo %>	
	<%= post.description %>
	<%= link_to 'View Post', post_path(post.id) %>
	<% if post.user_id == current_user %>
	    <%= link_to 'Edit Post', edit_post_path(post.id) %>
	    <%= link_to 'Delete', post_path(post.id), method: :delete %>
	    
	<% end %>
<% end %>